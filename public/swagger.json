			},
			"ValidateError": {
				"description": "Classe customizada para erros 400 ( Bad Request )\r\n\r\nSeu construtor possui:\r\n     message: Mensagem mostrado no erro\r\n     trace: Código de erro para restreio ao debuggar\r\n     stack: Caminho do erro ( opcional )",
				"properties": {
					"name": {
						"type": "string"
					},
					"message": {
						"type": "string"
					},
					"stack": {
						"type": "string"
					},
					"statusCode": {
						"$ref": "#/components/schemas/HttpStatusCode"
					},
					"trace": {
						"type": "string"
					}
				},
				"required": [
					"name",
					"message",
					"trace"
				],
				"type": "object",
				"additionalProperties": false,
				"example": {
					"message": "Campo é obrigatório.",
					"trace": "EXP9999",
					"stack": null
				}
			},
			"UnauthorizedError": {
				"description": "Classe customizada para erros 401 ( unauthorized )\r\nSua mensagem é sempre a mesma",
				"properties": {
					"name": {
						"type": "string"
					},
					"message": {
						"type": "string"
					},
					"stack": {
						"type": "string"
					},
					"statusCode": {
						"$ref": "#/components/schemas/HttpStatusCode"
					},
					"trace": {
						"type": "string"
					}
				},
				"required": [
					"name",
					"message",
					"trace"
				],
				"type": "object",
				"additionalProperties": false,
				"example": {
					"message": "Ação não autorizada.",
					"trace": "EXP9999"
				}
			},
			"NotFoundError": {
				"description": "Classe customizada para erros 404 ( Not Found )\r\n *\r\nSeu construtor possui:\r\n     message: Mensagem mostrado no erro\r\n     trace: Código de erro para restreio ao debuggar\r\n     stack: Caminho do erro ( opcional )",
				"properties": {
					"name": {
						"type": "string"
					},
					"message": {
						"type": "string"
					},
					"stack": {
						"type": "string"
					},
					"statusCode": {
						"$ref": "#/components/schemas/HttpStatusCode"
					},
					"trace": {
						"type": "string"
					}
				},
				"required": [
					"name",
					"message",
					"trace"
				],
				"type": "object",
				"additionalProperties": false,
				"example": {
					"message": "Item não encontrado.",
					"trace": "EXP9999",
					"stack": null
				}
			},
			"SignupDto": {
				"properties": {
					"username": {
						"type": "string"
					}
				},
				"required": [
					"username"
				],
				"type": "object",
				"additionalProperties": false,
				"example": {
					"username": "john_doe"
				}
			}
		},
		"securitySchemes": {
			"Bearer": {
				"type": "apiKey",
				"name": "Authorization",
				"in": "header"
			}
		}
	},
	"info": {
		"title": "chat-socket-nodejs",
		"version": "1.0.0",
		"license": {
			"name": "ISC"
		},
		"contact": {}
	},
	"paths": {
		"/user/signup": {
			"post": {
				"operationId": "signup",
				"responses": {
					"200": {
						"description": "Ok",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/UserCreateData"
								},
								"examples": {
									"Example 1": {
										"value": {
											"username": "john_doe",
											"createdAt": "2025-02-27T02:35:04.742Z"
										}
									}
								}
							}
						}
					},
					"400": {
						"description": "Bad Request",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/ValidateError"
								}
							}
						}
					},
					"401": {
						"description": "Unauthorized",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/UnauthorizedError"
								}
							}
						}
					},
					"404": {
						"description": "Not Found",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/NotFoundError"
								}
							}
						}
					}
				},
				"tags": [
					"Usuário"
				],
				"security": [],
				"parameters": [],
				"requestBody": {
					"required": true,
					"content": {
						"application/json": {
							"schema": {
								"$ref": "#/components/schemas/SignupDto"
							}
						}
					}
				}
			}
		}
	},
	"servers": [
		{
			"url": "http://localhost:4000/api/v1",
			"description": "Local development"
		}
	]
}